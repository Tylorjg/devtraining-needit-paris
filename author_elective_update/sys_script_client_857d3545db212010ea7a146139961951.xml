        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }
	alert("Hello");
   //Type appropriate comment here, and begin script below
	var getEmailAddr = GlideAjax('GetEmailAddress');
	getEmailAddr.addParam('sysparm_name', 'getEmail');
	getEmailAddr.addParam('sysparm_userID', g_form.getValue('u_requested_for'));
	getEmailAddr.getXML(updateEmail);
	
	function updateEmail(response) {
		var responseEmail = response.responseXML.documentElement.getAttribute("answer");
		g_form.clearValue('u_requested_for_email');
		g_form.setValue('u_requested_for_email', responseEmail);
	}
   
}]]></script>
